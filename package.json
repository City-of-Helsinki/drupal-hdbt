    "md5": "^2.3.0",
    "mini-css-extract-plugin": "^2.0.0",
    "mustache": "^4.2.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.27",
    "postcss-cli": "^11.0.0",
    "postcss-easy-import": "^4.0.0",
    "postcss-import": "^16.0.0",
    "postcss-import-ext-glob": "^2.0.1",
    "postcss-loader": "^8.0.0",
