<h4 class="errand-service__detail-title">{{ title }}</h4>
<ul class="errand-service__detail-links">
  {% for link in links %}
    <li class="errand-service__detail-link-wrapper">
      {% set link_attributes = {
        'class': [
          'errand-service__detail-link',
        ],
      } %}
      {{ link(link.title, link.url, link_attributes) }}
    </li>
  {% endfor %}
</ul>
